<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="448dp"
        android:layout_height="700dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menuimage" />

    <ImageButton
        android:id="@+id/ask"
        android:layout_width="40dp"
        android:layout_height="46dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ask" />

    <ImageButton
        android:id="@+id/btnLogout"
        android:layout_width="40dp"
        android:layout_height="46dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/ask"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logouticon" />

    <ImageButton
        android:id="@+id/websitebtn"
        android:layout_width="40dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/websiteicon" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="484dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView">

        <ImageButton
            android:id="@+id/notebook"
            android:layout_width="116dp"
            android:layout_height="108dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="48dp"
            android:layout_marginBottom="8dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cases"
            app:layout_constraintVertical_bias="0.578"
            app:srcCompat="@drawable/notebook" />

        <ImageButton
            android:id="@+id/media"
            android:layout_width="116dp"
            android:layout_height="108dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="8dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/chars"
            app:layout_constraintVertical_bias="0.529"
            app:srcCompat="@drawable/media" />

        <ImageButton
            android:id="@+id/chars"
            android:layout_width="116dp"
            android:layout_height="108dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="44dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/story"
            app:srcCompat="@drawable/chars" />

        <ImageButton
            android:id="@+id/cases"
            android:layout_width="116dp"
            android:layout_height="108dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="51dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/locations"
            app:srcCompat="@drawable/cases" />

        <ImageButton
            android:id="@+id/locations"
            android:layout_width="106dp"
            android:layout_height="104dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="48dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/location" />

        <ImageButton
            android:id="@+id/story"
            android:layout_width="116dp"
            android:layout_height="108dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="36dp"
            android:background="@null"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/story" />
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>